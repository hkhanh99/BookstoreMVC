@{
    @if (User.Identity.IsAuthenticated && User.IsInRole("Admin"))
    {
        Layout = "_LayoutAdmin";
        ViewData["Title"] = "Admin Page";

    }
    else if (User.Identity.IsAuthenticated && User.IsInRole("StoreOwner"))
    {
        Layout = "_Layout2";
        ViewData["Title"] = "StoreOwner Page";

    }
    else
    {
        Layout = "_Layout";
        ViewData["Title"] = "Normal Page";
    }

}